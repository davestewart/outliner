  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "chokidar": "^3.5.2",
    "cli-table3": "^0.6.0",
    "colors": "^1.4.0",
    "left-pad": "^1.3.0",
    "makerjs": "^0.17.1",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
