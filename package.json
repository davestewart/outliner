{
  "name": "@davestewart/outliner",
  "version": "1.1.0",
  "description": "A node package to outline SVG strokes as fills",
  "main": "src/index.js",
  "bin": {
    "outliner": "./bin/index.js"
  },
  "scripts": {
    "demo": "node main.js demo/source demo/target --autosize",
